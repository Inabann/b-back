  },
  "properties": {
    "admin": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "email": {
      "type": "string",
      "required": false
    },
    "saldo": {
      "type": "number",
      "required": false,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
